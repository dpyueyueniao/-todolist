s/TodoItem.vue'\nimport Compile from '../components/Compile.vue'\nexport default {\n  name: 'HelloWorld', \n  components: {\n    TodoItem: TodoItem, \n    Compile: Compile\n  }, \n  data () {\n    return {\n      ipt_content: '', \n      todolist: JSON.parse(localStorage.getItem('todolist')) || [], \n      sec: {\n        show: false,\n        index: NaN,\n        new_content: '' \n      }\n    }\n  }, \n  methods: {\n    handelNewList: function() {\n      if(this.ipt_content){\n        this.todolist.push(this.ipt_content), \n        localStorage.setItem('todolist', JSON.stringify(this.todolist)), \n        this.ipt_content = ''\n      }else{\n        alert('内容不能为空')\n      }\n    },\n    itemdel: function(index) {\n      this.todolist.splice(index, 1), \n      localStorage.setItem('todolist', JSON.stringify(this.todolist))\n    }, \n    itemadd: function(i, newitem){\n      this.sec.show = true\n      this.sec.index = i\n      this.sec.new_content = newitem\n      this.compileKey = Math.random()\n    }, \n    secNew: function(i, newitem) {\n      this.todolist.splice(i, 1, newitem)\n      this.sec.show = false\n      localStorage.setItem('todolist', JSON.stringify(this.todolist))\n    }\n  }\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\n.home_box{\n  width: 100%;\n  align-items: center;\n  .home{\n    text-align: center;\n  }\n}\n\n.head_title{\n  margin: 3rem 0 3rem;\n  font-size: 1.5rem;\n}\n.home_ipt{\n  width: 20rem;\n  height: 2rem;\n}\n.home_but{\n  margin-left: 1rem;\n  color: rgba(189, 42, 42, 0.979);\n  width: 6rem;\n  height: 2.6rem;\n  background-color: rgba(148, 150, 151, 0.651);\n  border: 1px solid rgba(3, 2, 2, 0.904);\n  border-radius: .2rem;\n}\n.home_but:hover {\n    background-color: rgba(148, 150, 151, 0.651); \n    border: 1px solid rgba(189, 42, 42, 0.979);\n    color: white;\n}\n</style>\n"]}]}